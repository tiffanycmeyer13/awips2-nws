<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.3"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
       <handler
            class="gov.noaa.nws.ocp.viz.cwagenerator.CwaStartHandler"
            commandId="gov.noaa.nws.ocp.viz.cwa.ui.Cwa">
       </handler>    
   </extension>
   
   <extension
         point="org.eclipse.ui.commands">
          <command
            id="gov.noaa.nws.ocp.viz.cwa.ui.Cwa"
            name="CWA"> 
          </command>     
   </extension>
   
   <extension
       point="org.eclipse.ui.menus">
       <menuContribution locationURI="menu:tools?before=tools.Da">
            <command
                commandId="gov.noaa.nws.ocp.viz.cwa.ui.Cwa"
                label="CWA Generator">
                <visibleWhen>
                    <reference
                        definitionId="com.raytheon.uf.viz.d2d.ui.inD2DActionSet">
                    </reference>
                </visibleWhen>
            </command>
        </menuContribution>
   </extension>
   <extension
       point="com.raytheon.uf.viz.localization.perspective.localizationpath">
    <path
          application="CWA Generator"
          localizationType="COMMON_STATIC"
          name="config"
          recursive="true"
          value="cwsu">
    </path>
 </extension>
</plugin>