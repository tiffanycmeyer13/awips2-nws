<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="odimDataCubeAdapter" class="gov.noaa.nws.ocp.viz.plugin.odim.ODIMDataCubeAdapter" />

    <bean id="odimDataCubeAdapterRegistered"
        factory-bean="dataCubeAdapterRegistry"
        factory-method="registerAdapter">
        <constructor-arg ref="odimDataCubeAdapter" />
    </bean>

</beans>