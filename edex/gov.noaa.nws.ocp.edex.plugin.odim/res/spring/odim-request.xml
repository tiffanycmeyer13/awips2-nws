<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="odimMenuUtil" class="gov.noaa.nws.ocp.edex.plugin.odim.menus.ODIMMenuUtil" />
    <bean id="odimMenus" factory-bean="menuCreationRegistry" factory-method="register">
        <constructor-arg value="odim"/>
        <constructor-arg ref="odimMenuUtil"/>
    </bean>

</beans>
